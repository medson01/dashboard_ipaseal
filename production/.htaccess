# Bloqueando hotlinks em imagens
RewriteEngine on
RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http(s)?://(www\.)?sistemas/dashboard_ipaseal2/ [NC]

RewriteRule \.(jpg|jpeg|png|gif)$ - [NC,F,L]

# Protegendo seu arquivo .htaccess
<files ~ "^.*\.([Hh][Tt][Aa])">
order allow,deny
deny from all
satisfy all
</files>

# Evitando a listagem de diret√≥rios 
Options -Indexes

